<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<update handle="customer_account"/>
<head>
    <title>Set Zaproo Status: Yes/No</title>
</head>
<body>
    <referenceContainer name="content">
        <block class="Magento\Framework\View\Element\Template" name="header.mini.container" template="Funami_Zaproo::html/header.phtml"/>

        <block as="zaproostatus" class="Funami\Zaproo\Block\ZaprooStatus" name="zaproostatus" template="Funami_Zaproo::zaproostatus.phtml"/>
        <block class="Magento\Customer\Block\Account\Dashboard" name="customer.account.link.back" template="Magento_Customer::account/link/back.phtml" cacheable="false"/>
    </referenceContainer>
</body>
</page>
